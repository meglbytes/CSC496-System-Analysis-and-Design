USE csc420_meglama

SELECT	FirstName,
LastName,
CarID,
RentalStartDate,
RentalEndDate,
FlightID,
Destination,
DepartureDate,
ReturnDate,
FlightReservationID,
HotelID,
ArrivalDate,
DepartureDate,
HotelReservationID

FROM Cars C
AND CarReservations CR
AND Customers Cu
AND Flights F
AND FlightReservations FR
AND Hotel H
AND HotelReservations HR

WHERE Cu.CustomerID = CR.CarReservationID
AND CR.CarID = C.CarID
AND Cu.CustomerID = FR.CustomerID
AND FR.FlightID = F.FlightID
AND Cu.CustomerID = HR.CustomerID
AND HR.HotelID = H.HotelID